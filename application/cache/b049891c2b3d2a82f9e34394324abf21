<script type="text/javascript">
$(document).ready(function(){
	//更多选项展开/收起
	$(".er_more").click(function() {
		var display = 'none';
		var url = 'http://192.168.1.105/ysw_front/index.php/cooperation/index/keywords/aa';
		if(display == 'block')
		{
			$(".wrapper").slideToggle(function(){
				location.href = url+'/display/none';
			});
			$('.zk').html('更多选项（项目类型）');
		}
		else
		{
			$(".wrapper").slideToggle(function(){
				location.href = url+'/display/block';
			});
			$('.zk').html('收起');
		}
	});

	//展开多选效果
	$('.btn').click(function(){
		var display = $(this).css('display');
		if(display == 'block')
		{
			$('.er_nav_off').hide();
			$('.er_nav_on').show();
			$(this).hide();
		}
	});

	//取消多选效果
	$('.cancel').click(function(){
		$('.er_nav_off').show();
		$('.er_nav_on').hide();
		$('.btn').show();
	});

	//提交多选属性
	$('.submit').click(function(){
		var url = 'http://192.168.1.105/ysw_front/index.php/cooperation/index/keywords/aa';
		var str='';
		$(".er_nav_on label input[type=checkbox]").each(function(){
			if(this.checked){
				str += $(this).val()+'-';
			}
		});
		location.href = url +'/checkbox/'+ str;
		$('.btn').hide();
	});

	//打开页面判断是否选择有属性
	$('.er_xuan').each(function(){
		if($(this).find('.on').length=='0')
		{
			$('.er_xuan').hide();
		}
	});

	//合作收藏
	$('.sc').click(function(){
		var uuid ='a7423342-75f5-11e4-ae65-52540039b119';
		//alert(uuid);
		if($.trim(uuid).length != 0)
		{
			var task_uuid = $(this).attr('rel');
			$.get("http://192.168.1.105/ysw_front/index.php/cooperation/collect"+'/'+task_uuid,function(data){
				if(data == 'true')
				{
					alert('成功添加收藏！');
				}
				else
				{
					alert('不可重复收藏！');
				}
			});
		}
		else
		{
			alert('登录之后才可以收藏次合作！');
		}
	});

	//列表鼠标移过变化背景颜色、显示收藏按钮
	$(".coop_nr").hover(function(){
		$(this).find(".sc").show();
		$(this).addClass("coo_lbj");
	},function(){
		$(".coop_nr .sc").hide();
		$(".coop_nr").removeClass("coo_lbj");
	});
});
</script><div class="footer">

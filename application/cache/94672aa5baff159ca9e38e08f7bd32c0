<script type="text/javascript">
$(document).ready(function(){
	//下拉登录框
	$("#jq_topmenu li").hover(function(){
		$(this).addClass("hover").find("div.jq_hidebox").show();
	},function(){
		$(this).removeClass("hover").find("div.jq_hidebox").hide();
	});

	//搜索
	$('#searchbtn').click(function(){
		var site = 'http://192.168.1.105/ysw_front/index.php';
		var type = $('#searchtype').attr('typename');
		var keywords = $('#searchkey').val();
		if(keywords !='')
		{
			$('#top_searchform').attr('action',site + '/' + type + '/index/keywords/' + keywords);
		}
		else
		{
			$('#top_searchform').attr('action',site + '/' + type);
		}
	});
});
</script><div class="main">
